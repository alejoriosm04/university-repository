/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
        Nand(a=a, b=a, out=NotA);
        Nand(a=b, b=b, out=NotB);
        Nand(a=NotA, b=NotB, out=out);
}
