/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
        Not(in=sel, out=notsel);
        And(a=a, b=notsel, out=checka);
        And(a=b, b=sel, out=checkb);
        Or(a=checka, b=checkb, out=out);
}