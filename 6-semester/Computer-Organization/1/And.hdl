/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    //// Replace this comment with your code.
        Nand(a=a, b=b, out=NandAB);
        // Nand (a=NandAB, b=NandAB, out=out);
        Not(in=NandAB, out=out);
}